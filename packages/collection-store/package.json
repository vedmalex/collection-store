{
  "name": "collection-store",
  "version": "5.0.0-distributed-enterprise",
  "description": "Enterprise-grade distributed collection store with Write-Ahead Logging (WAL), Multi-Node Replication, ACID transactions, B+ Tree indexing, and industry-leading performance (90K+ ops/sec, 0.06ms network latency)",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "bun": "./src/index.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./types/index.d.ts",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "prepublish": "npm run clean && npm run build",
    "build": "npm run build:all",
    "watch": "tsc -w -p tsconfig.json",
    "clean": "git clean -dfqX -- {types,dist,node_modules}/** && rm -rf tsconfig.tsbuildinfo dist types",
    "build:esm": "TOOL=esbuild FORMAT=esm bun build.ts",
    "build:cjs": "TOOL=esbuild FORMAT=cjs bun build.ts",
    "build:bun:esm": "TOOL=bun FORMAT=esm bun build.ts",
    "build:bun:cjs": "TOOL=bun FORMAT=cjs bun build.ts",
    "build:bun": "npm run build:bun:cjs && npm run build:bun:esm",
    "build:all": "npm run build:bun && tsc -p tsconfig.build.json",
    "test:wal": "bun test src/__test__/wal-*.test.ts",
    "test:advanced": "bun test src/__test__/advanced-features.test.ts",
    "test:performance": "bun test src/__test__/performance-benchmarks.test.ts",
    "test:stress": "bun test src/__test__/stress-testing.test.ts",
    "test:replication": "bun test src/__test__/replication-*.test.ts",
    "demo:replication": "bun run src/demo/replication-demo.ts",
    "demo:wal-streaming": "bun run src/demo/wal-streaming-demo.ts"
  },
  "files": [
    "dist",
    "types",
    "WAL_QUICK_START_GUIDE.md",
    "FINAL_PROJECT_COMPLETION.md",
    "REPLICATION_SYSTEM_PLAN.md",
    "REPLICATION_PHASE1_COMPLETE.md",
    "REPLICATION_PHASE2_COMPLETE.md",
    "REPLICATION_PROGRESS_SUMMARY.md",
    "REPLICATION_TESTS_IMPROVEMENT.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/vedmalex/collection-store.git"
  },
  "type": "module",
  "keywords": [
    "database",
    "collection",
    "wal",
    "write-ahead-logging",
    "transactions",
    "acid",
    "b-tree",
    "indexing",
    "typescript",
    "enterprise",
    "performance",
    "mongodb",
    "nosql",
    "in-memory",
    "persistence",
    "crash-recovery",
    "durability",
    "compression",
    "monitoring",
    "replication",
    "distributed",
    "cluster",
    "consensus",
    "raft",
    "websocket",
    "high-availability",
    "failover",
    "multi-master",
    "master-slave"
  ],
  "author": "vedanta-krit.das <vedmalex@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vedmalex/collection-store/issues"
  },
  "homepage": "https://github.com/vedmalex/collection-store",
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cron": "^2.4.0",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.10",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.15.21",
    "@types/ws": "^8.5.13",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "eslint": "^9.27.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-watch": "^8.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "ts-jest": "^29.3.4",
    "tsd": "^0.32.0",
    "typescript": "next"
  },
  "dependencies": {
    "b-pl-tree": "^1.3.0",
    "bcrypt": "^5.1.1",
    "cron": "^4.3.0",
    "fs-extra": "^11.3.0",
    "jsondiffpatch": "^0.7.3",
    "jsonwebtoken": "^9.0.2",
    "lodash-es": "^4.17.21",
    "ws": "^8.18.0",
    "zod": "^3.25.28",
    "zod-validation-error": "^3.4.1"
  },
  "gitHead": "6be7bf4855f9224dda0b6b8fb97230af8a3296f1"
}
