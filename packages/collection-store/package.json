{
  "name": "collection-store",
  "version": "3.0.0-beta.23",
  "description": "very flexible collection for in-memory operations with flexible indexing",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "bun": "./src/index.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./types/index.d.ts",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "prepublish": "npm run clean && npm run build",
    "build": "npm run build:all",
    "watch": "tsc -w -p tsconfig.json",
    "clean": "git clean -dfqX -- {types,dist,node_modules}/** && rm -rf tsconfig.tsbuildinfo dist types",
    "build:esm": "TOOL=esbuild FORMAT=esm bun build.ts",
    "build:cjs": "TOOL=esbuild FORMAT=cjs bun build.ts",
    "build:bun:esm": "TOOL=bun FORMAT=esm bun build.ts",
    "build:bun:cjs": "TOOL=bun FORMAT=cjs bun build.ts",
    "build:bun": "npm run build:bun:cjs && npm run build:bun:esm",
    "build:all": "npm run build:bun && tsc -p tsconfig.build.json"
  },
  "files": [
    "dist",
    "types"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/vedmalex/collection-store.git"
  },
  "type": "module",
  "keywords": [],
  "author": "vedanta-krit.das <vedmalex@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vedmalex/collection-store/issues"
  },
  "homepage": "https://github.com/vedmalex/collection-store",
  "devDependencies": {
    "@types/cron": "^2.4.0",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.10",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.15.21",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "eslint": "^9.27.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-watch": "^8.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "ts-jest": "^29.3.4",
    "typescript": "next"
  },
  "dependencies": {
    "b-pl-tree": "^1.2.5",
    "cron": "^4.3.0",
    "fs-extra": "^11.3.0",
    "jsondiffpatch": "^0.7.3",
    "lodash-es": "^4.17.21",
    "zod": "^3.25.28",
    "zod-validation-error": "^3.4.1"
  },
  "gitHead": "6be7bf4855f9224dda0b6b8fb97230af8a3296f1"
}
