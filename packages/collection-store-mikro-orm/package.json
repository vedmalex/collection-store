{
  "name": "collection-store-mikro-orm",
  "version": "3.0.0-beta.22",
  "description": "TypeScript ORM for Node.js based on Data Mapper, Unit of Work and Identity Map patterns. Supports MongoDB, MySQL, PostgreSQL and SQLite databases as well as usage with vanilla JavaScript.",
  "exports": {
    ".": {
      "bun": "./src/index.ts",
      "default": "./dist/index.js"
    }
  },
  "types": "./src/index.ts",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/mikro-orm/mikro-orm.git"
  },
  "keywords": [
    "orm",
    "mongo",
    "mongodb",
    "mysql",
    "mariadb",
    "postgresql",
    "sqlite",
    "sqlite3",
    "ts",
    "typescript",
    "js",
    "javascript",
    "entity",
    "ddd",
    "mikro-orm",
    "unit-of-work",
    "data-mapper",
    "identity-map"
  ],
  "type": "module",
  "author": "Martin AdÃ¡mek",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mikro-orm/mikro-orm/issues"
  },
  "homepage": "https://mikro-orm.io",
  "engines": {
    "node": ">= 18.12.0"
  },
  "scripts": {
    "prepublish": "bun build.ts && bun run compile",
    "clean": "git clean -dfqX -- ./node_modules **/{dist,node_modules}/",
    "compile": "tsc --build tsconfig.json",
    "compile:clean": "tsc --build tsconfig.json --clean",
    "watch": "tsc --build tsconfig.json --watch",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "collection-store": "workspace:*"
  },
  "devDependencies": {
    "@mikro-orm/core": "^6.1.5",
    "typescript": "next",
    "vite-plugin-babel": "^1.2.0"
  },
  "peerDependencies": {
    "@mikro-orm/core": "^6.1.5"
  },
  "gitHead": "14ec69d5fce5f0c48d93dc3afcfcb6232c4ffaaa"
}
